x=\"/home\">\r\n                    <i class=\"el-icon-menu\"></i><span slot=\"title\">首页</span>\r\n                </el-menu-item>\r\n                <el-submenu index=\"2\">\r\n                    <template slot=\"title\">\r\n                        <i class=\"el-icon-location\"></i>\r\n                        <span slot=\"title\">财务管理</span>\r\n                    </template>\r\n                    <el-menu-item-group>\r\n                        <el-menu-item index=\"/memberOrder\">\r\n                            <i class=\"el-icon-s-order\"></i>\r\n                            <span slot=\"title\">会员订单</span>\r\n                        </el-menu-item>\r\n                    </el-menu-item-group>\r\n                </el-submenu>\r\n                <el-submenu index=\"3\">\r\n                    <template slot=\"title\">\r\n                        <i class=\"el-icon-s-platform\"></i>\r\n                        <span slot=\"title\">库存管理</span>\r\n                    </template>\r\n                    <el-menu-item-group>\r\n                        <el-menu-item index=\"/stockQuery\">\r\n                            <i class=\"el-icon-s-order\"></i>\r\n                            <span slot=\"title\">库存查询</span>\r\n                        </el-menu-item>\r\n                    </el-menu-item-group>\r\n                </el-submenu>\r\n                <!-- <el-menu-item index=\"/user\">\r\n                    <i class=\"el-icon-s-check\"></i\r\n                    ><span slot=\"title\">用户管理</span>\r\n                </el-menu-item> -->\r\n            </el-menu>\r\n        </div>\r\n\r\n        <!-- 主区域 -->\r\n        <div class=\"main\">\r\n            <el-breadcrumb separator-class=\"el-icon-arrow-right\" v-show=\"$route.path !='/'\">\r\n                <el-breadcrumb-item v-show=\"$route.path=='/memberOrder' || $route.path== '/bookinfo'\">财务管理</el-breadcrumb-item>\r\n                <el-breadcrumb-item v-show=\"$route.path=='/stockQuery' || $route.path== '/kucunsearch